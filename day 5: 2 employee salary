total=0
grade=input("enter grade:")
sal=int(input("enter salary:"))
if sal<0:
    print("invalid input")
else:
    if grade=='A' and sal>10000:
        bonus=0.05*sal
        total=sal+bonus
    elif grade=='B' and sal>10000:
        bonus=0.1*sal
        total=sal+bonus
    elif grade=='A' and sal<=10000:
        bonus=0.07*sal
        total=sal+bonus
    elif grade=='B' and sal<=10000:
        bonus=0.12*sal
        stotal=sal+bonus
    print("total",total)
